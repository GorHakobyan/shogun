
	.SEGMENT "RECORD"


	.FUNCT	V-$COMMAND:ANY:0:0
	DIRIN	D-COMMAND-FILE
	RTRUE	


	.FUNCT	V-$RANDOM:ANY:0:0
	EQUAL?	PRSO,INTNUM /?CCL3
	PRINTR	"Illegal call to #RANDOM."
?CCL3:	SUB	0,P-NUMBER
	RANDOM	STACK
	RTRUE	


	.FUNCT	V-$RECORD:ANY:0:0
	DIROUT	D-RECORD-ON
	RTRUE	


	.FUNCT	V-$UNRECORD:ANY:0:0
	DIROUT	D-RECORD-OFF
	RTRUE	

	.ENDSEG

	.ENDI
